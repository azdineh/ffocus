{
  "name": "ffocus",
  "version": "0.1.1",
  "description": "A CLI tool to temporarly disable distracting websites and boost your productivity",
  "repository": "git://github.com/giuseppeg/ffocus",
  "main": "dist/ffocus.js",
  "files": [
    "dist/ffocus.js",
    "README.md"
  ],
  "bin": {
    "ffocus": "dist/ffocus.js"
  },
  "scripts": {
    "prebuild": "mkdir -p dist",
    "build": "babel bin/ffocus.js -o dist/ffocus.js",
    "prepublish": "npm run build",
    "test": "exit(0)"
  },
  "keywords": [
    "focus",
    "procrastination",
    "productivity"
  ],
  "author": "Giuseppe Gurgone",
  "license": "MIT",
  "dependencies": {
    "commander": "^2.9.0",
    "hostile": "^1.1.1",
    "update-notifier": "^1.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.18.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  }
}
